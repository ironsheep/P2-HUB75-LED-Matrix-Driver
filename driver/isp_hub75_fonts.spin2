CON
  
    FONT_8x8_BYTES_PER_CHAR = 8
    FONT_5x7_BYTES_PER_CHAR = 7

PUB null()      '' This is not a top level object

PUB getAddrFor8x8Char(asciiByte) : p8x8CharBitmap   
    '' Return the address of an 8x8 character bitmap
    p8x8CharBitmap := 0
    if(asciiByte >= $20 && asciiByte <= $7f)
        p8x8CharBitmap := @byte[@font][(asciiByte - $20) * FONT_8x8_BYTES_PER_CHAR]

PUB getAddrFor5x7Char(asciiByte) : p5x7CharBitmap
    '' Return the address of a 5x7 character bitmap
    p5x7CharBitmap := 0
    if(asciiByte >= $20 && asciiByte <= $7f)
        p5x7CharBitmap := @byte[@font5x7][(asciiByte - $20) * FONT_5x7_BYTES_PER_CHAR]

