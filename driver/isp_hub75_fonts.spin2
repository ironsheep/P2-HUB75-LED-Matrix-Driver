CON { Object Interface: PUBLIC ENUMs }

' supported fonts:
#0, TEXT_FONT_DEFAULT, TEXT_FONT_5x7, TEXT_FONT_8x8A, TEXT_FONT_8x8B

    FONT_8x8A_BYTES_PER_CHAR = 8
    FONT_8x8B_BYTES_PER_CHAR = 8
    FONT_5x7_BYTES_PER_CHAR = 7

PUB null()      
    '' This is not a top level object

PUB getCharAddrForFont(selectdFont, cChar) : pCharBitmap
    '' Return address of char from specified font
    case selectdFont
        TEXT_FONT_DEFAULT..TEXT_FONT_5x7:
            pCharBitmap := getAddrFor5x7Char(cChar)
        TEXT_FONT_8x8A:
            pCharBitmap := getAddrFor8x8aChar(cChar)
        TEXT_FONT_8x8B:
            pCharBitmap := getAddrFor8x8bChar(cChar)

PUB getAddrFor8x8aChar(asciiByte) : p8x8CharBitmap   
    '' Return the address of an 8x8 (a) character bitmap
    p8x8CharBitmap := 0
    if(asciiByte >= $20 && asciiByte <= $7f)
        ' NOTE: add + 1 to skip top blank row of char
        p8x8CharBitmap := @byte[@font8x8a][((asciiByte - $20) * FONT_8x8A_BYTES_PER_CHAR) + 1]

PUB getAddrFor8x8bChar(asciiByte) : p8x8CharBitmap   
    '' Return the address of an 8x8 (b) character bitmap
    if(asciiByte < $20 || asciiByte > $7f)
        asciiByte := $20
    p8x8CharBitmap := @byte[@font8x8b][asciiByte * FONT_8x8B_BYTES_PER_CHAR]

PUB getAddrFor5x7Char(asciiByte) : p5x7CharBitmap
    '' Return the address of a 5x7 character bitmap
    p5x7CharBitmap := 0
    if(asciiByte >= $20 && asciiByte <= $7f)
        p5x7CharBitmap := @byte[@font5x7][(asciiByte - $20) * FONT_5x7_BYTES_PER_CHAR]

